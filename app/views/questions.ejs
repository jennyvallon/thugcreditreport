<form>    <% for(var i=0; i < questions.length; i++) { %>        <question-module>            <p><%= questions[i].text %></p>            <ul>                <% for(var j=0; j < questions[i].answers.length; j++) { %>                <li>                    <input                         type ="<%= questions[i].inputType %>"                         data-score ="<%= questions[i].score %>"                         name ="answer"                         value ="<%= questions[i].answers[j].text %>"                    >                     <%= questions[i].answers[j].text %>                </li><br>                <% } %>            </ul>        </question-module>    <% } %>    <input type="submit" value="Submit"></form>