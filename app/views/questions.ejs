<% for(var i=0; i < questions.length; i++) { %>    <form>          <question-module>            <p><%= questions[i].text %></p>            <ul>                <% for(var j=0; j < questions[i].answers.length; j++) { %>                <li>                    <input                         type ="<%= questions[i].inputType %>"                         name ="answer"                         value ="<%= questions[i].answers[j].text %>"                        data-question="<%= [i] %>"                        data-answer="<%= [j] %>"                    >                     <%= questions[i].answers[j].text %>                </li><br>                <% } %>            </ul>        </question-module>    </form><% } %><input id='submit' type="submit" value="Submit"><script   src="https://code.jquery.com/jquery-3.0.0.min.js"   integrity="sha256-JmvOoLtYsmqlsWxa7mDSLMwa6dZ9rrIdtrrVYRnDRH0="   crossorigin="anonymous"></script><script src="../js/questions.js" type="text/javascript"></script>