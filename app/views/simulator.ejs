<!DOCTYPE html><html xmlns:ng="http://angularjs.org">    <head>        <title>THUG REPORT|SIMULATOR</title>        <meta charset="UTF-8">        <meta name="viewport" content="width=device-width, initial-scale=1.0">    </head>    <body>               <score-simulator>            <users-scores>                <current-score><%=thugScore %></current-score>                <new-score></new-score>            </users-scores>            <actions-module>                <action-slider ng-view></action-slider>                <actions-chooser>                    <form>                        <input type="radio" name="simulatedAction" data-url="snitch" > Snitch<br>                        <input type="radio" name="simulatedAction" data-url="movework"> Move Work<br>                        <input type="radio" name="simulatedAction" data-url="evadepolice"> Evade Police                    </form>                </actions-chooser>            </action-module>         </score-simulator>                <script type="text/javascript" src="/lib/jquery/dist/jquery.min.js"></script>        <script type="text/javascript" src="/lib/angular/angular.js"></script>        <script type="text/javascript" src="/lib/angular-route/angular-route.js"></script>        <script type="text/javascript" src="/example/example.client.module.js"></script>        <script type="text/javascript" src="/example/controllers/example.client.controller.js"></script>        <script type="text/javascript" src="/example/controllers/evadepolice.client.controller.js"></script>        <script type="text/javascript" src="/example/controllers/movework.client.controller.js"></script>        <script type="text/javascript" src="/example/controllers/snitch.client.controller.js"></script>        <script type="text/javascript" src="/example/config/example.client.routes.js"></script>        <script type="text/javascript" src="/application.js"></script>    </body></html>