<!DOCTYPE html><html>    <head>        <title>THUG REPORT | Sign In</title>        <link rel="stylesheet" type="text/css" href="./css/styles.css">    </head>    <body data-activePage="signin">        <page-container>            <% for(var i in messages) { %>              <div class="flash"><%= messages[i] %></div>            <% } %>            <form action="/signin" method="post">                <div>                    <label>Username:</label>                    <input type="text" name="username" />                </div>                <div>                    <label>Password:</label>                    <input type="password" name="password" />                </div>                 <div>                    <input type="submit" value="Sign In" />                </div>       ï¿¼    </form>        </page-container>    </body></html>